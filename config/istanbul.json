{
  "verbose": false,
  "instrumentation": {
    "root": ".",
    "default-excludes": true,
    "excludes": [],
    "embed-source": false,
    "variable": "__coverage__",
    "compact": true,
    "preserve-comments": false,
    "complete-copy": false,
    "save-baseline": false,
    "baseline-file": "./coverage/coverage-baseline.json",
    "include-all-sources": false
  },
  "reporting": {
    "print": "summary",
    "reports": ["lcov"],
    "dir": "./coverage",
    "watermarks": {
      "statements": [50, 80],
      "lines": [50, 80],
      "functions": [50, 80],
      "branches": [50, 80]
    },
    "report-config": {
      "json": {
        "file": "coverage-final.json"
      },
      "json-summary": {
        "file": "coverage-summary.json"
      },
      "lcovonly": {
        "file": "lcov.info"
      }
    }
  },
  "hooks": {
    "hook-run-in-context": false,
    "post-require-hook": null,
    "handle-sigint": false
  },
  "check": {
    "global": {
      "statements": 20,
      "lines": 20,
      "branches": 10,
      "functions": 20,
      "excludes": []
    }
  }
}
